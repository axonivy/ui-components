{
  "private": true,
  "name": "parent",
  "engines": {
    "node": ">=20.10.0"
  },
  "volta": {
    "node": "20.10.0"
  },
  "scripts": {
    "ci": "npm install && npm run generate && npm run package && npm run type && npm run lint:ci && npm run test:ci",
    "generate": "npm run generate --workspace=@axonivy/ui-icons",
    "build": "lerna run build",
    "build:storybook": "storybook build",
    "package": "lerna run package",
    "clean": "lerna run clean",
    "type": "lerna run type",
    "lint": "lerna run lint --",
    "lint:ci": "npm run lint -- -o eslint.xml -f checkstyle",
    "lint:fix": "lerna run lint:fix --",
    "dev": "storybook dev -p 6006 --no-open",
    "test": "npm run test --workspace=@axonivy/ui-components",
    "test:ci": "lerna run test:ci",
    "publish:next": "lerna publish --exact --canary --preid next --pre-dist-tag next --no-git-tag-version --no-push --ignore-scripts --yes"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/manager-api": "^8.3.1",
    "@storybook/react": "^8.3.1",
    "@storybook/react-vite": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/theming": "^8.3.1",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vanilla-extract/css": "^1.15.4",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/vite-plugin": "^4.0.14",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "lerna": "^8.1.8",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.3.1",
    "typescript": "~5.5.4",
    "typescript-cp": "^0.1.9",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
